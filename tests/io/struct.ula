estructura Circle
	radius: entero
	x: entero
	y: entero = 99

cir: Circle = Circle(radius=5, x=2)
anotherCir = Circle(radius=1, x=3)

mostrar(anotherCir.x)
mostrar(cir.radius + cir.x)
cir.x = 20
mostrar(cir.x)
mostrar(cir.y)

def giveMeACircle(some: Circle) -> Circle
    some.y = -1
    retorna some

a_circle = giveMeACircle(cir)
mostrar(a_circle.x)
mostrar(a_circle.y)
mostrar(a_circle es Circle)